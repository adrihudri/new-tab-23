{
  "manifest_version": 3,
  "name": "Better New Tab",
  "description": "Replace new tab with a custom page.",
  "version": "1.0",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "topSites",
    "storage",
    "activeTab",
    "bookmarks"
  ],
  "host_permissions": [
    "https://api.pexels.com/*"
  ],
  "icons": {
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }

}
